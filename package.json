  "description": "",
  "type": "module",
  "scripts": {
    "start": "tsx watch index.ts",
    "generate:graphql": "graphql-codegen --config codegen.ts && bash scripts/makeResolversStrict.sh",
    "generate:supabase": "supabase gen types typescript --project-id 'mncmkeesizmphvqylfqr' --schema public > src/generated/supabase.ts",
    "generate": "pnpm run generate:graphql && pnpm run generate:supabase",
    "start:prod": "pnpm run generate && tsx index.ts",
    "check:ts": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "check": "pnpm run check:ts && pnpm run format && pnpm run lint && vitest run",
    "test": "vitest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "^4.9.5",
    "@supabase/supabase-js": "^2.39.2",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "date-fns": "^3.0.6",
    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "graphql-modules": "^2.3.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
